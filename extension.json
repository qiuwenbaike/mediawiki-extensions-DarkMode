{
	"name": "DarkMode",
	"version": "0.4.0",
	"author": [ "MusikAnimal", "R4356th", "安忆", "WaitSpring" ],
	"url": "https://git.qiuwen.net.cn/Fork/mediawiki-extensions-DarkMode",
	"descriptionmsg": "darkmode-desc",
	"license-name": "GPL-3.0",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\DarkMode\\": "includes/"
	},
	"Hooks": {
		"BeforePageDisplay": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\DarkMode\\Hooks",
			"services": [ "MainConfig" ]
		}
	},
	"MessagesDirs": {
		"DarkMode": [ "i18n" ]
	},
	"ResourceModules": {
		"ext.DarkMode": {
			"packageFiles": [ "ext.DarkMode.js" ],
			"messages": [
				"darkmode-link",
				"darkmode-link-tooltip",
				"darkmode-default-link",
				"darkmode-default-link-tooltip"
			]
		},
		"ext.DarkMode.EmojiWrap": {
			"es6": true,
			"packageFiles": [ "ext.DarkMode.EmojiWrap.js" ]
		},
		"ext.DarkMode.AddBackground": {
			"es6": true,
			"packageFiles": [ "ext.DarkMode.AddBackground.js" ]
		},
		"ext.DarkMode.css": {
			"styles": [ "ext.DarkMode.less" ]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "DarkMode/resources"
	},
	"manifest_version": 2
}
